<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC '-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN' 'http://java.sun.com/dtd/web-app_2_3.dtd'>
<web-app id="Events">

    <display-name>DeviceEvents</display-name>
    <description>Device formatted events</description>

    <servlet>
        <servlet-name>Events</servlet-name>
        <servlet-class>org.opendmtp.war.events.Events</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Events</servlet-name>
        <url-pattern>*.kml</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Events</servlet-name>
        <url-pattern>*.csv</url-pattern>
    </servlet-mapping>

</web-app>
